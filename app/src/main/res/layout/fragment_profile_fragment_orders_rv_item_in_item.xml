<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="@dimen/fragment_profile_fragment_orders_rv_item_in_item_padding">

    <RelativeLayout
        android:id="@+id/fragment_profile_addresses_item_rv_item_in_item_RL_container"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/fragment_profile_addresses_item_rv_item_in_item_RL_container_padding"
        >
        <TextView
            android:id="@+id/fragment_profile_fragment_addresses_rv_item_in_item_count_of_items_for_order"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"/>
        <TextView
            android:id="@+id/fragment_profile_fragment_addresses_rv_item_in_item_times"
            android:layout_marginStart="@dimen/fragment_profile_fragment_addresses_rv_item_in_item_times_marginsStart"
            android:layout_marginEnd="@dimen/fragment_profile_fragment_addresses_rv_item_in_item_times_marginEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/fragment_profile_fragment_addresses_rv_item_in_item_count_of_items_for_order"
            android:text="@string/fragment_profile_fragment_addresses_rv_item_in_item_times"/>
        <TextView
            android:id="@+id/fragment_profile_fragment_addresses_rv_item_in_item_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@id/fragment_profile_fragment_addresses_rv_item_in_item_times"
           />

        <TextView
            android:id="@+id/fragment_profile_fragment_addresses_rv_item_in_item_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toStartOf="@+id/fragment_profile_fragment_addresses_rv_item_in_item_price_type"
            />
        <TextView
            android:layout_marginStart="@dimen/fragment_profile_fragment_addresses_rv_item_in_item_price_type_marginStart"
            android:id="@+id/fragment_profile_fragment_addresses_rv_item_in_item_price_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:text="@string/fragment_profile_fragment_addresses_rv_item_in_item_price_type"/>
    </RelativeLayout>
</LinearLayout>